<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>

  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br>

  <%= f.label :genre %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <br>

  <label>Song Notes 1</label>
  <%= f.text_field :notes_1 %>

  <label>Song Notes 2</label>
  <%= f.text_field :notes_2 %>


  <%= f.submit %>

<% end %>


